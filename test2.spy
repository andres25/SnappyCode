INICIOPROGRAMA
	crear entero global1;
	crear flotante global2;
	crear entero arrGlobal[150];
	crear entero despues;


	INICIOFUNCION flotante factorial PARAMETROS entero n
		crear flotante x;
		SI (n==0) ENTONCES
			INICIOBLOQUE
				x = 1;
		 	FINBLOQUE
		SINO
		 	INICIOBLOQUE
		 		x = n*factorial(n-1);
		 	FINBLOQUE
		FINSI
		
		REGRESA x;
	FINFUNCION

	INICIOFUNCION entero fiboAux PARAMETROS entero n
		crear entero x;
		crear entero fib1;
		crear entero fib2;
		crear entero fibRes;

		SI (n>2) ENTONCES
			INICIOBLOQUE
				fib1 = fiboAux(n-1);
		 		fib2 = fiboAux(n-2);
		 		x = fib1 + fib2;
		 	FINBLOQUE
		FINSI
		SI (n==2) ENTONCES
		 	INICIOBLOQUE
		 		x=1;
		 	FINBLOQUE
		FINSI
		SI (n==1) ENTONCES
		 	INICIOBLOQUE
		 		x=1;
		 	FINBLOQUE
		FINSI
		SI (n==0) ENTONCES
		 	INICIOBLOQUE
		 		x=0;
		 	FINBLOQUE
		FINSI

		REGRESA x;
	FINFUNCION

	INICIOFUNCION entero fibonacci PARAMETROS entero n
		crear entero fibInt;
		crear entero i;
		
		i = 0;
		MIENTRAS i<n HACER
	    	INICIOBLOQUE
		        fibInt = fiboAux(i);
		        DECIRALUSUARIO(fibInt);
		        i = i + 1;
	    	FINBLOQUE
		FINMIENTRAS

		REGRESA i;
	FINFUNCION

	INICIOPRINCIPAL 
		crear flotante fact;
		crear entero fib;
		crear entero indice;
		crear entero arreglo[5];

		arreglo[3]=5;

		fib = fibonacci(5);
		DECIRALUSUARIO(fib);
		indice = 0;
		MIENTRAS indice < 5 HACER
			INICIOBLOQUE
				SI (arreglo[indice] == 5) ENTONCES
					INICIOBLOQUE
						DECIRALUSUARIO(indice);
					FINBLOQUE
				FINSI
				indice = indice +1;
			FINBLOQUE
		FINMIENTRAS
	FINPRINCIPAL 

FINPROGRAMA